<div class="modal fade" id="#imageModal-{{this.id_barang}}" tabindex="-1" data-bs-backdrop="static" aria-labelledby="imageModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="imageModalLabek">Tampil Gambil Barang</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>

      {{#if this.img_barang}}
      <div class="modal-body">
        <img src="upload/img/{{this.img_barang}}" alt="" srcset="">
      </div>
      {{else}}
      <div class="modal-body">
        <img src="asset/img/default-picture.png" alt="" srcset="">
      </div>
      {{/if}}

      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>